#inclui "Conta.h"

 void Conta::sacar(float valorASacar){
    if(valorASacar < 15){
		cout << "Ops! Não é possível sacar valores abaixo de R$15,00" << endl;
		}else if(valorASacar > saldo){
			cout << "Ops! Você nãoo possui saldo suficiente." << endl;
		}else{
			saldo -= valorASacar;
		}
 }

 void Conta::depositar(float valorADepositar){
    if(valorADepositar < 10){
			cout << "Ops! N�o � poss�vel depositar valores abaixo de R$10,00" << endl;
		}else{
			saldo += valorADepositar;
		}
 }
